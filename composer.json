{
  "name": "texlab/texlab",
  "description": "The Texlab PHP framework",
  "license": "MIT",
  "require": {
    "php": "^7.2",
    "texlab/mydb": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "config": {
    "process-timeout": 0
  },
  "scripts": {
    "start": "php -S localhost:8080 -t public",
    "scss": "sass --watch public/scss:public/css --no-source-map --style compressed"
  }
}